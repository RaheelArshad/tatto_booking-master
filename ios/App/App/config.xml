  </feature>

  <feature name="NativeGeocoder">
    <param name="ios-package" value="NativeGeocoder"/>
  </feature>

  <feature name="NetworkStatus">
    <param name="ios-package" value="CDVConnection"/>
  </feature>

  <feature name="SignInWithApple">
    <param name="ios-package" value="SignInWithApple"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="CordovaStripe">
    <param name="ios-package" value="CordovaStripe"/>
  </feature>

  <feature name="SocialSharing">
    <param name="ios-package" value="SocialSharing"/>
    <param name="onload" value="true"/>
